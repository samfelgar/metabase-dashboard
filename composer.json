{
    "name": "samfelgar/metabase-dashboard",
    "description": "Laravel Nova tool for embedding Metabase dashboards",
    "keywords": [
        "laravel",
        "nova",
        "metabase",
        "dashboard"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0",
        "laravel/nova": "^4.0",
        "lcobucci/jwt": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Samfelgar\\MetabaseDashboard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Samfelgar\\MetabaseDashboard\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
